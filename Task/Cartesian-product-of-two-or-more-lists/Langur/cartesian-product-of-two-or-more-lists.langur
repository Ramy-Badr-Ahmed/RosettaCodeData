val X = fn ...x:x

writeln mapX(X, [1, 2], [3, 4]) == [[1, 3], [1, 4], [2, 3], [2, 4]]
writeln mapX(X, [3, 4], [1, 2]) == [[3, 1], [3, 2], [4, 1], [4, 2]]
writeln mapX(X, [1, 2], []) == []
writeln mapX(X, [], [1, 2]) == []
writeln()

writeln mapX(X, [1776, 1789], [7, 12], [4, 14, 23], [0, 1])
writeln()

writeln mapX(X, [1, 2, 3], [30], [500, 100])
writeln()

writeln mapX(X, [1, 2, 3], [], [500, 100])
writeln()
