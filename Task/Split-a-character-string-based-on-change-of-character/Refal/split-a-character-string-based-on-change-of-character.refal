$ENTRY Go {
    = <Prout <Join (', ') <Split 'gHHH5YY++///\\'>>>;
};

Split {
    (e.Cur) = (e.Cur);
    (e.Cur s.1) s.1 e.X = <Split (e.Cur s.1 s.1) e.X>;
    (e.Cur) s.1 e.X = (e.Cur) <Split (s.1) e.X>;
    s.1 e.X = <Split (s.1) e.X>;
};

Join {
    (e.Joiner) = ;
    (e.Joiner) (e.Str) = e.Str;
    (e.Joiner) (e.Str) e.Strs = e.Str e.Joiner <Join (e.Joiner) e.Strs>;
};
